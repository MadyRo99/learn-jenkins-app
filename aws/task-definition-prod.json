{
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "family": "LearnJenkinsApp-TaskDefinition-Prod",
  "containerDefinitions": [
    {
      "name": "LearnJenkinsApp",
      "image": "980921727519.dkr.ecr.eu-central-1.amazonaws.com/learnjenkinsapp:1.0.16",
      "portMappings": [{
        "name": "ngnix-80-tip",
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp",
        "appProtocol": "http"
      }],
      "essential": true
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::980921727519:role/ecsTaskExecutionRole"
}